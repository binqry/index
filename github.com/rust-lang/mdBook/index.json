{
  "meta": {
    "url-format": "https://github.com/rust-lang/mdBook/releases/download/v{{.Version}}/mdbook-v{{.Version}}-{{.Arch}}-{{.OS}}{{.Ext}}",
    "replacements": {
      "amd64": "x86_64",
      "darwin": "apple-darwin",
      "linux": "unknown-linux-gnu",
      "windows": "pc-windows-msvc"
    },
    "extension": {
      "default": ".tar.gz",
      "windows": ".zip"
    }
  },
  "latest": { "version": "0.4.0" },
  "versions": [
    { "version": "0.4.0" },
    { "version": "0.3.7" }
  ]
}